uppers <- LETTERS[1:26]
lowers <- letters[1:26]
numbers <- seq(0,9)
catDecider <- function(x) {
#check <- x
if(x %in% lowers){
return(1)
}
else if (x %in% uppers){
return(2)
}
else if (x %in% numbers) {
return(3)
}
return (4)
}
